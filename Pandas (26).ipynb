    "# Input\n",
    "fruit = pd.Series(np.random.choice(['apple', 'banana', 'carrot'], 10))\n",
    "weights = pd.Series(np.linspace(1, 10, 10))\n",
    "\n",
    "# Solution\n",
    "weights.groupby(fruit).mean()\n",
    "\n",
    "# output\n",
    "apple     7.4\n",
    "banana    2.0\n",
    "carrot    6.0\n",
    "dtype: float64"
   ]
